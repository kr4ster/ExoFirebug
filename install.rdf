	<Description about="urn:mozilla:install-manifest">
		<em:id>exofirebug@exosuite.org</em:id>
		<em:version>0.1</em:version>
		<em:type>2</em:type>
		<em:name>ExoFirebug</em:name>
		<em:description>An extension to Firebug that supports ExoWeb</em:description>
